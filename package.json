{
	"name": "portfolio",
	"private": true,
	"version": "0.1.8",
	"type": "module",
	"homepage": "/app",
	"scripts": {
		"dev": "vite",
		"generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
		"build": "yarn generate-meta-tag && vite build",
		"serve": "serve dist",
		"lint": "eslint 'src/**/*.{js,jsx}' --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prepare": "husky install",
		"svgs": "npx @svgr/cli -d src/assets/svgs/components src/assets/svgs --filename-case kebab",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"dependencies": {
		"@sentry/react": "^8.45.0",
		"@sentry/vite-plugin": "^3.3.1",
		"@tanstack/react-query": "^5.62.7",
		"@tanstack/react-query-devtools": "^5.62.7",
		"axios": "^1.7.9",
		"change-object-case": "^0.2.1",
		"cross-env": "^7.0.3",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.13",
		"formik": "^2.4.6",
		"lodash": "^4.17.21",
		"lucide-react": "^0.474.0",
		"react": "^19.0.0",
		"react-cache-buster": "^0.1.8",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.0.2",
		"react-toastify": "^10.0.6",
		"yup": "^1.5.0"
	},
	"devDependencies": {
		"@svgr/cli": "^8.1.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@vitejs/plugin-react-swc": "^3.7.2",
		"@vitest/coverage-v8": "3.1.2",
		"eslint": "^9.16.0",
		"eslint-define-config": "^2.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.16",
		"eslint-plugin-unused-imports": "^4.1.4",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"lint-staged": "^15.2.11",
		"prettier": "3.4.2",
		"sass": "^1.83.0",
		"serve": "^14.2.4",
		"vite": "^6.0.3",
		"vite-plugin-svgr": "^4.3.0",
		"vitest": "^3.1.2"
	}
}
